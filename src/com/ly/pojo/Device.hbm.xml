<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.ly.pojo">
    <class name="Device" table="device_">
        <id name="id" column="id">
            <generator class="native">
            </generator>
        </id>
        <property name="name" />
        <property name="htype" />
        <property name="stype" />
        <property name="mid" />
        <property name="oid" />
        <property name="location" />
        <property name="status" />
        <property name="bdate" />
    </class>
     
</hibernate-mapping>